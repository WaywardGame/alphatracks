{
    "compilerOptions": {
        "target": "es2017",
        "module": "amd",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "inlineSourceMap": true,
        "baseUrl": "./",
        "paths": {
            "*": ["node_modules/@wayward/types/definitions/*"],
            "@wayward/goodstream": [  "node_modules/@wayward/goodstream/Stream" ],
            "@wayward/goodstream/*": [ "node_modules/@wayward/goodstream/*" ]
        },
        "types": [],
        "outDir": "out",
        "experimentalDecorators": true
    }
}